generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model FormData {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  email     String
  formData  Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@unique([email])
}
